<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.jcryptool.visual"
            class="org.jcryptool.visual.androidUnlockPattern.views.AndroidUnlockPattern"
            icon="icons/view.gif"
            id="org.jcryptool.visual.androidUnlockPattern.views.AndroidUnlockPattern"
            name="%view.name">
      </view>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.jcryptool.core.operations.visuals">
      <Visual
            name="%Visual.name"
            viewId="org.jcryptool.visual.androidUnlockPattern.views.AndroidUnlockPattern">
      </Visual>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.jcryptool.visual.androidUnlockPattern.views.AndroidUnlockPattern">
         <command
               commandId="org.jcryptool.visual.androidUnlockPattern.RestartCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/clear_co.gif"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.jcryptool.visual.androidUnlockPattern.views.AndroidUnlockPattern">
         <command
               commandId="org.jcryptool.visual.androidUnlockPattern.RestartCommand"
               icon="platform:/plugin/org.eclipse.ui/icons/full/etool16/clear_co.gif"
               style="push"
               tooltip="%command.tooltip">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="org.jcryptool.visual.androidUnlockPattern.RestartCommand"
            name="%command.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.jcryptool.visual.androidUnlockPattern.handlers.RestartHandler"
            commandId="org.jcryptool.visual.androidUnlockPattern.RestartCommand">
      </handler>
   </extension>

</plugin>
